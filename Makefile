# MAKE
# ------------------------------------------------------------------------------------------
TARGET := result

PHONY	:=	all
all:

# CODING-STYLE
# ------------------------------------------------------------------------------------------
PHONY	:=	fmt
fmt:
	black $(TARGET) && isort $(TARGET)

PHONY	:=	lint
lint:
	mypy $(TARGET) && pylint $(TARGET)

# etc...
# ------------------------------------------------------------------------------------------
.PHONY: $(PHONY)
